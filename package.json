{
  "name": "36-40-fullstack-app",
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__test__/mocks/styleMock.js",
      "\\.(css|less|scss)$": "<rootDir>/src/__test__/mocks/styleMock.js"
    }
  },
  "version": "1.0.0",
  "description": "image-galleria-app",
  "main": "index.js",
  "engines": {
    "node": "10.4.1",
    "npm": "6.3.0-next.0"
  },
  "scripts": {
    "test": "cypress run --browser chrome",
    "watch": "webpack-dev-server --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "lint": "eslint --fix **/*.js",
    "cypress:open": "cypress open",
    "ci-start": "http-server ./build -a localhost -p 8080 -c-1 &",
    "start": "node index.js",
    "pretest": "npm run build && npm run ci-start && wait-on http://localhost:8080",
    "posttest": "pkill -f http-server",
    "heroku-prebuild": "npm uninstall cypress && npm run build",
    "heroku-postbuild": "webpack --progress --config webpack.prod.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^1.0.0",
    "cypress": "^3.0.3",
    "eslint": "^5.0.0-rc.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-cypress": "^2.0.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-react": "^7.10.0",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "jest": "^23.4.0",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.9.2",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-router-dom": "^4.3.1",
    "redux-devtools-extension": "^2.13.5",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "wait-on": "^2.1.0",
    "webpack": "^4.11.1",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "nodemon": "^1.18.3",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "superagent": "^3.8.3",
    "validator": "^10.5.0",
    "webpack-cli": "^3.0.8",
    "webpack-merge": "^4.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LizabethPetersen/36-40-fullstack-app.git"
  },
  "bugs": {
    "url": "https://github.com/LizabethPetersen/36-40-fullstack-app/issues"
  },
  "homepage": "https://github.com/LizabethPetersen/36-40-fullstack-app#readme"
}
